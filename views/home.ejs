


<header>
    <div id="header">
        <p>Issue Tracker</p>
        <button id="create-project"><a href="/project/createproject">create project</a></button>
    </div>

</header>
<div  id="homePage">
    <h1>Project Details</h1>
        
        <% for(i of project){ %>
           <div id="project-list" >
            <h3><%= i.projectname%></h3>
            <div id="project-info">
                <p>Project Author :<%= i.projectdescription%></p>
                <p>Project Description :<%= i.projectdescription%></p>
            </div>
            <button id="show-issue">
                <a href="/project/issue?projectid=<%=i.id%>"><i class='fas fa-eye' style='font-size:24px'></i> Show Issue</a>
            </button>
           </div> 
                
            
        <% } %> 
    

</div>


